{
  "id": "cdc64feb-bcd3-4675-be78-44f8d6f2af9d",
  "name": "Top: External Attackers",
  "description": "<DescripciÃ³n de la consulta>",
  "mode": "single",
  "index": ["logs-*"],
  "aggregation_result": true,
  "type": "1",
  "query": {
    "aggs": {
      "2": {
        "terms": {
          "field": "originIp",
          "size": 10,
          "order": {
            "_count": "desc"
          }
        }
      }
    },
    "size": 0,
    "_source": {
      "excludes": []
    },
    "stored_fields": ["*"],
    "script_fields": {},
    "docvalue_fields": [
      {
        "field": "insertedDate",
        "format": "date_time"
      },
      {
        "field": "logDate",
        "format": "date_time"
      },
      {
        "field": "normalDate",
        "format": "date_time"
      }
    ],
    "query": {
      "bool": {
        "must": [
          {
            "range": {
              "normalDate": {
                "gte": "{{gte}}",
                "lte": "{{lte}}",
                "format": "epoch_millis"
              }
            }
          }
        ],
        "filter": [
          {
            "bool": {
              "should": [{ "match": { "entityId": "{{entity_ids}}" } }],
              "minimum_should_match": 1
            }
          },
          {
            "bool": {
              "filter": [
                {
                  "bool": {
                    "should": [{ "match": { "msgClassName": "attack" } }],
                    "minimum_should_match": 1
                  }
                },
                {
                  "bool": {
                    "filter": [
                      {
                        "bool": {
                          "should": [
                            { "match": { "directionName": "external" } }
                          ],
                          "minimum_should_match": 1
                        }
                      },
                      {
                        "bool": {
                          "should": [{ "match": { "isEvent": true } }],
                          "minimum_should_match": 1
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        ],
        "should": [],
        "must_not": []
      }
    }
  }
}
