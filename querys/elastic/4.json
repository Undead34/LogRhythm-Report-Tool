{
  "id": "classification_name_count",
  "metadata": {
    "name": "Clasificación Nombre Recuento",
    "description": "Clasificación nombre recuento"
  },
  "processing_policy": {
    "mode": "single",
    "index": [
      "logs-*"
    ],
    "date_range_replacement": {
      "field": "normalDate",
      "gte_placeholder": "{{gte}}",
      "lte_placeholder": "{{lte}}",
      "format": "epoch_millis"
    },
    "entity_ids_replacement": {
      "placeholder": "{{entity_ids}}",
      "type": "query_string",
      "field": "entityId"
    }
  },
  "result_processing": {
    "columns": [
      "msgClassName",
      "count"
    ],
    "visualizations": []
  },
  "query": {
    "size": 0,
    "query": {
      "bool": {
        "must": [
          {
            "query_string": {
              "query": "{{entity_ids}}",
              "analyze_wildcard": true,
              "default_field": "*"
            }
          },
          {
            "range": {
              "normalDate": {
                "gte": "{{gte}}",
                "lte": "{{lte}}",
                "format": "epoch_millis"
              }
            }
          }
        ]
      }
    },
    "aggs": {
      "msgclassname_counts": {
        "terms": {
          "field": "msgClassName",
          "size": 500,
          "order": {
            "_count": "desc"
          }
        }
      }
    }
  }
}