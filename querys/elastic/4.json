{
  "id": "337fb1d3-a5be-4f20-9928-f662ae002910",
  "name": "Events in the month",
  "description": "<DescripciÃ³n de la consulta>",
  "mode": "single",
  "index": ["logs-*"],
  "aggregation_result": true,
  "type": "5",
  "query": {
    "size": 0,
    "query": {
      "bool": {
        "must": [
          {
            "query_string": {
              "query": "{{entity_ids}}",
              "analyze_wildcard": true,
              "default_field": "*"
            }
          },
          {
            "range": {
              "normalDate": {
                "gte": "{{gte}}",
                "lte": "{{lte}}",
                "format": "epoch_millis"
              }
            }
          }
        ]
      }
    },
    "aggs": {
      "msgClassName_counts": {
        "terms": {
          "field": "msgClassName",
          "size": 500,
          "order": {
            "_count": "desc"
          }
        }
      }
    }
  }
}
